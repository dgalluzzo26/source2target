{
  "name": "source2target",
  "version": "1.0.0",
  "description": "Databricks app with Vue frontend and FastAPI backend",
  "scripts": {
    "install": "cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "uvicorn backend.app:app --host 0.0.0.0 --port ${DATABRICKS_APP_PORT:-8000}",
    "dev": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn app:app --reload"
  },
  "keywords": ["databricks", "vue", "fastapi"],
